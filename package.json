{
  "name": "ci-cd",
  "version": "1.0.0",
  "main": "index.js",
  "author": "luscabap <lucasbaptistasilva.dev@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "eslint-plugin-prettier": "^5.2.1",
    "next": "^14.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "scripts": {
    "dev": "next dev",
    "build": "rm -rf out && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "deploy": "yarn build && npx vercel ./out --yes",
    "deploy:staging": "yarn build && yarn deploy --token=$VERCEL_TOKEN",
    "deploy:prod": "yarn deploy:staging --prod",
    "deploy:preview": "node ./scripts/pullRequestPreview"
  },
  "devDependencies": {
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3"
  }
}
